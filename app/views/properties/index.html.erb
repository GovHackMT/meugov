<p id="notice"><%= notice %></p>

<h1>Listing Properties</h1>

<table>
  <thead>
    <tr>
      <th>Property category</th>
      <th>Name</th>
      <th>Street</th>
      <th>Number</th>
      <th>Complement</th>
      <th>Neighborhood</th>
      <th>Cep</th>
      <th>City</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.property_category %></td>
        <td><%= property.name %></td>
        <td><%= property.street %></td>
        <td><%= property.number %></td>
        <td><%= property.complement %></td>
        <td><%= property.neighborhood %></td>
        <td><%= property.cep %></td>
        <td><%= property.city %></td>
        <td><%= property.latitude %></td>
        <td><%= property.longitude %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
