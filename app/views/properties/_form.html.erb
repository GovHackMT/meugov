<%= simple_form_for(@property) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :property_category %>
    <%= f.input :name %>
    <%= f.input :street %>
    <%= f.input :number %>
    <%= f.input :complement %>
    <%= f.input :neighborhood %>
    <%= f.input :cep %>
    <%= f.association :city %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
