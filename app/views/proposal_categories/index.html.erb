<p id="notice"><%= notice %></p>

<h1>Listing Proposal Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proposal_categories.each do |proposal_category| %>
      <tr>
        <td><%= proposal_category.name %></td>
        <td><%= link_to 'Show', proposal_category %></td>
        <td><%= link_to 'Edit', edit_proposal_category_path(proposal_category) %></td>
        <td><%= link_to 'Destroy', proposal_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proposal category', new_proposal_category_path %>
